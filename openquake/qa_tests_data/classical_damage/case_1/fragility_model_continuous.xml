<?xml version="1.0" encoding="utf-8"?>
<nrml
xmlns="http://openquake.org/xmlns/nrml/0.5"
xmlns:gml="http://www.opengis.net/gml"
>
    <fragilityModel
    format="continuous"
    >
        <description>
            Fragility model for Nepal
        </description>
        <limitStates>
            slight moderate extensive complete
        </limitStates>
        <ffs
        noDamageLimit="0.05"
        type="lognormal"
        >
            <taxonomy>
                W
            </taxonomy>
            <IML
            IMT="PGA"
            imlUnit="g"
            maxIML="1.4"
            minIML="0.05"
            >
                PGA None 0.05 1.4 g
            </IML>
            <ffc
            ls="slight"
            >
                <params
                mean="0.160"
                stddev="0.104"
                >
                    0.16 0.104
                </params>
            </ffc>
            <ffc
            ls="moderate"
            >
                <params
                mean="0.225"
                stddev="0.158"
                >
                    0.225 0.158
                </params>
            </ffc>
            <ffc
            ls="extensive"
            >
                <params
                mean="0.400"
                stddev="0.300"
                >
                    0.4 0.3
                </params>
            </ffc>
            <ffc
            ls="complete"
            >
                <params
                mean="0.600"
                stddev="0.48"
                >
                    0.6 0.48
                </params>
            </ffc>
        </ffs>
    </fragilityModel>
</nrml>
