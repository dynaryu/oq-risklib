<?xml version="1.0" encoding="utf-8"?>
<nrml
xmlns="http://openquake.org/xmlns/nrml/0.5"
xmlns:gml="http://www.opengis.net/gml"
>
    <fragilityModel
    format="continuous"
    >
        <description>
            Fragility model for QA test
        </description>
        <limitStates>
            LS1 LS2
        </limitStates>
        <ffs
        type="lognormal"
        >
            <taxonomy>
                RC
            </taxonomy>
            <IML
            IMT="PGA"
            imlUnit="g"
            maxIML="9.9"
            minIML="0.1"
            >
                PGA None 0.1 9.9 g
            </IML>
            <ffc
            ls="LS1"
            >
                <params
                mean="0.2"
                stddev="0.05"
                >
                    0.2 0.05
                </params>
            </ffc>
            <ffc
            ls="LS2"
            >
                <params
                mean="0.35"
                stddev="0.10"
                >
                    0.35 0.1
                </params>
            </ffc>
        </ffs>
        <ffs
        type="lognormal"
        >
            <taxonomy>
                RM
            </taxonomy>
            <IML
            IMT="PGA"
            imlUnit="g"
            maxIML="9.9"
            minIML="0.1"
            >
                PGA None 0.1 9.9 g
            </IML>
            <ffc
            ls="LS1"
            >
                <params
                mean="0.25"
                stddev="0.08"
                >
                    0.25 0.08
                </params>
            </ffc>
            <ffc
            ls="LS2"
            >
                <params
                mean="0.40"
                stddev="0.12"
                >
                    0.4 0.12
                </params>
            </ffc>
        </ffs>
    </fragilityModel>
</nrml>
