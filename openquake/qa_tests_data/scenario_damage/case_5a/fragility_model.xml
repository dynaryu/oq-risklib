<?xml version="1.0" encoding="utf-8"?>
<nrml
xmlns="http://openquake.org/xmlns/nrml/0.5"
xmlns:gml="http://www.opengis.net/gml"
>
    <fragilityModel
    format="continuous"
    >
        <description>
            Fragility Functions for Case 1d
        </description>
        <limitStates>
            slight moderate extreme complete
        </limitStates>
        <ffs
        type="lognormal"
        >
            <taxonomy>
                Wood
            </taxonomy>
            <IML
            IMT="PGA"
            imlUnit="g"
            maxIML="5.0"
            minIML="0.0"
            >
                PGA None 0.0 5.0 g
            </IML>
            <ffc
            ls="slight"
            >
                <params
                mean="0.50"
                stddev="0.40"
                >
                    0.5 0.4
                </params>
            </ffc>
            <ffc
            ls="moderate"
            >
                <params
                mean="1.00"
                stddev="0.80"
                >
                    1.0 0.8
                </params>
            </ffc>
            <ffc
            ls="extreme"
            >
                <params
                mean="1.50"
                stddev="1.20"
                >
                    1.5 1.2
                </params>
            </ffc>
            <ffc
            ls="complete"
            >
                <params
                mean="2.00"
                stddev="1.60"
                >
                    2.0 1.6
                </params>
            </ffc>
        </ffs>
    </fragilityModel>
</nrml>
